{
  "builds": [
    {
      "src": "gestaoclick/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "gestaoclick/produtos.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "gestaoclick"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/produtos",
      "dest": "/gestaoclick/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/gestaoclick/produtos.html"
    }
  ]
}
